{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": [
      "ES2020"
    ],
    "types": [
      "@cloudflare/workers-types"
    ],
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "sourceMap": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}